    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.3",
    "v8": "^0.1.0",
